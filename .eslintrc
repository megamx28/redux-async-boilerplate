{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "__DEV__": false,
    "__PROD": false,
    "__DEBUG__": false,
    "it": false,
    "expect": false
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolve": 0,
    "comma-dangle": 0,
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": 0,
    "arrow-body-style": 0,
    "react/prefer-stateless-function": 0,
    "no-var": 0,
    "global-require": 0,
    "prefer-const": 0,
    "no-unused-vars": 0,
    "object-shorthand": 0,
    "react/react-in-jsx-scope": 0,
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "react/forbid-prop-types": 0,
    "no-plusplus": 0,
    "react/jsx-boolean-value": 0
  }
}
