{
  "name": "redux-async-boilerplate",
  "version": "1.0.0",
  "description": "Redux Async Boilerplate",
  "homepage": "https://github.com/shanedasilva",
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedasilva/redux-async-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shanedasilva/redux-async-boilerplate/issues"
  },
  "scripts": {
    "dev": "node -r dotenv/config ./bin/server.js",
    "lint": "./node_modules/eslint/bin/eslint.js ./src/ --ext .js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 9001",
    "storybook:deploy": "storybook-to-ghpages",
    "compile": "NODE_ENV=production DEBUG=app:* webpack -p --config ./config/webpack.prod.config.js --progress",
    "deploy": "npm run test && rimraf ./dist && npm run compile"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "@kadira/storybook-deployer": "^1.2.0",
    "classnames": "^2.2.5",
    "debug": "^2.2.0",
    "dotenv": "2.0.0",
    "history": "4.3.0",
    "lodash": "4.16.4",
    "lodash.isplainobject": "^4.0.6",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-api-middleware": "^1.0.2",
    "redux-logger": "^2.7.4",
    "redux-thunk": "2.1.0",
    "superagent": "^2.3.0",
    "yargs": "6.3.0"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@kadira/storybook": "2.28.1",
    "babel-core": "6.18.0",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "0.25.0",
    "enzyme": "2.5.1",
    "eslint": "3.9.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "6.4.1",
    "faker": "3.1.0",
    "file-loader": "0.9.0",
    "jest": "^16.0.2",
    "nock": "^9.0.2",
    "node-babel": "^0.1.2",
    "node-sass": "^3.10.1",
    "phantomjs-prebuilt": "^2.1.13",
    "react-addons-test-utils": "^15.3.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "rimraf": "^2.5.4",
    "sass-loader": "4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "0.13.1",
    "stylelint-webpack-plugin": "0.4.0",
    "url-loader": "^0.5.7",
    "watch": "1.0.1",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.1"
  }
}
