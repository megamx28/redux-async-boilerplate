{
  "name": "redux-async-boilerplate",
  "version": "0.0.0",
  "description": "Redux Async Boilerplate",
  "scripts": {
    "start": "npm run dev",
    "dev": "node -r dotenv/config ./bin/server.js",

    "lint": "eslint ./src/ --ext .js --ext .jsx",
    "lint:fix": "npm run lint -- --fix",

    "test": "NODE_ENV=production karma start ./bin/karma.js",
    "test:watch": "watch 'npm run test' ./tests/",
    "test:coverage": "npm run test -- --coverage",

    "doctor": "rm -rf node_modules && npm cache clean && npm install && npm run dev",

    "clean": "rimraf ./dist",
    "compile": "NODE_ENV=production DEBUG=app:* webpack -p --config ./config/webpack.prod.config.js --progress",
    "deploy": "npm run test && npm run clean && npm run compile",

    "storybook": "start-storybook -p 9001",
    "storybook:publish": "build-storybook -o storybook-build-dir"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedasilva/redux-async-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shanedasilva/redux-async-boilerplate/issues"
  },
  "homepage": "https://github.com/shanedasilva",
  "dependencies": {
    "classnames": "^2.2.1",
    "debug": "^2.2.0",
    "dotenv": "2.0.0",
    "history": "2.1.0",
    "humps": "1.1.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "4.11.1",
    "lodash.isplainobject": "^4.0.4",
    "normalizr": "2.0.1",
    "react": "15.0.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-test-utils": "15.0.1",
    "react-dom": "15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "2.3.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.1",
    "redux-api-middleware": "^1.0.2",
    "redux-form": "^5.3.1",
    "redux-logger": "^2.0.2",
    "redux-thunk": "2.0.1",
    "yargs": "4.6.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.41.0",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "eslint": "3.0.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "5.2.2",
    "expect": "^1.18.0",
    "file-loader": "0.9.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "karma": "1.1.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "0.7.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "1.0.1",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.0.4",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-babel": "^0.1.2",
    "node-sass": "^3.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "0.9.1",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.2.3",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.4.4",
    "sass-loader": "4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "0.19.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
