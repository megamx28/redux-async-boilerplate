{
  "name": "redux-async-boilerplate",
  "version": "1.0.0",
  "description": "Redux Async Boilerplate",
  "homepage": "https://github.com/shanedasilva/redux-async-boilerplate",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedasilva/redux-async-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shanedasilva/redux-async-boilerplate/issues"
  },
  "scripts": {
    "dev": "node -r dotenv/config ./bin/server.js",
    "flow": "flow check",
    "flow:coverage": "flow-coverage-report -i './src/**/*.js' -t html -t text",
    "lint": "./node_modules/eslint/bin/eslint.js ./src/ --ignore-pattern 'src/**/*.spec.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 9001",
    "storybook:deploy": "storybook-to-ghpages",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "compile": "NODE_ENV=production DEBUG=app:* webpack -p --config ./config/webpack.prod.config.js --progress",
    "build": "npm run test && rimraf ./dist && npm run compile",
    "precommit": "npm run lint && npm run test",
    "release": "semantic-release pre && semantic-release post"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "dependencies": {
    "@kadira/storybook-deployer": "^1.2.0",
    "classnames": "^2.2.5",
    "debug": "^2.6.0",
    "dotenv": "4.0.0",
    "history": "4.5.1",
    "lodash": "4.17.4",
    "lodash.isplainobject": "^4.0.6",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-api-middleware": "^1.0.2",
    "redux-logger": "^2.7.4",
    "redux-thunk": "2.2.0",
    "superagent": "^3.4.0",
    "yargs": "6.6.0"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@kadira/storybook": "2.35.3",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "commitizen": "^2.9.5",
    "css-loader": "0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "enzyme": "2.7.1",
    "eslint": "3.14.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "6.9.0",
    "faker": "3.1.0",
    "file-loader": "0.9.0",
    "flow-bin": "^0.38.0",
    "flow-coverage-report": "^0.3.0",
    "husky": "^0.13.1",
    "jest": "^18.1.0",
    "nock": "^9.0.2",
    "node-babel": "^0.1.2",
    "node-sass": "^4.3.0",
    "phantomjs-prebuilt": "^2.1.14",
    "react-addons-test-utils": "^15.4.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "rimraf": "^2.5.4",
    "sass-loader": "4.1.1",
    "semantic-release": "^6.3.2",
    "style-loader": "0.13.1",
    "stylelint-webpack-plugin": "0.5.1",
    "url-loader": "^0.5.7",
    "validate-commit-msg": "^2.11.1",
    "watch": "1.0.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0"
  }
}
