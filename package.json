{
  "name": "redux-async-boilerplate",
  "version": "0.0.0-development",
  "description": "Redux Async Boilerplate",
  "homepage": "https://github.com/shanedasilva/redux-async-boilerplate",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedasilva/redux-async-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shanedasilva/redux-async-boilerplate/issues"
  },
  "scripts": {
    "dev": "node -r dotenv/config ./bin/server.js",
    "flow": "flow check",
    "flow:coverage": "flow-coverage-report -i './src/**/*.js' -t html -t text",
    "lint": "./node_modules/eslint/bin/eslint.js ./src/ --ignore-pattern 'src/**/*.spec.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 9001",
    "storybook:deploy": "storybook-to-ghpages",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "compile": "NODE_ENV=production DEBUG=app:* webpack -p --config ./config/webpack.prod.config.js",
    "build": "npm run test && rimraf ./dist && npm run compile",
    "precommit": "npm run lint && npm run test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/tests/stubs/styles.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "dependencies": {
    "@kadira/react-storybook-addon-info": "^3.4.0",
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-deployer": "^1.2.0",
    "classnames": "^2.2.5",
    "debug": "^2.6.8",
    "dotenv": "4.0.0",
    "faker": "^4.1.0",
    "history": "4.6.3",
    "invariant": "^2.2.2",
    "lodash": "4.17.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "^5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.2.0",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "commitizen": "^2.9.6",
    "css-loader": "0.28.4",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "2.9.1",
    "eslint": "4.3.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "7.1.0",
    "file-loader": "0.11.2",
    "flow-bin": "^0.51.1",
    "flow-coverage-report": "^0.3.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "nock": "^9.0.14",
    "node-babel": "^0.1.2",
    "node-sass": "^4.5.3",
    "phantomjs-prebuilt": "^2.1.14",
    "react-addons-test-utils": "^15.6.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "rimraf": "^2.6.1",
    "sass-loader": "6.0.6",
    "semantic-release": "^6.3.6",
    "style-loader": "0.18.2",
    "url-loader": "^0.5.9",
    "validate-commit-msg": "^2.14.0",
    "watch": "1.0.2",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
