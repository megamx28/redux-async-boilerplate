{
  "name": "redux-async-boilerplate",
  "version": "0.0.0",
  "description": "Redux Async Boilerplate",
  "scripts": {
    "dev": "node -r dotenv/config ./bin/server.js",
    "lint": "eslint ./src/ --ext .js --ext .jsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=production karma start ./bin/karma.js",
    "test:watch": "watch 'npm run test' ./tests/",
    "test:coverage": "npm run test -- --coverage",
    "doctor": "rm -rf node_modules && npm cache clean && npm install && npm run dev",
    "clean": "rimraf ./dist",
    "compile": "NODE_ENV=production DEBUG=app:* webpack -p --config ./config/webpack.prod.config.js --progress",
    "deploy": "npm run test && npm run clean && npm run compile",
    "storybook": "start-storybook -p 9001",
    "storybook:publish": "build-storybook -o storybook-build-dir"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedasilva/redux-async-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shanedasilva/redux-async-boilerplate/issues"
  },
  "homepage": "https://github.com/shanedasilva",
  "dependencies": {
    "classnames": "^2.2.5",
    "debug": "^2.2.0",
    "dotenv": "2.0.0",
    "history": "3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "4.15.0",
    "lodash.isplainobject": "^4.0.6",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-api-middleware": "^1.0.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "2.1.0",
    "yargs": "5.0.0"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "@kadira/storybook": "2.3.0",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "3.3.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "6.1.2",
    "expect": "^1.20.2",
    "faker": "3.1.0",
    "file-loader": "0.9.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "karma": "1.2.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.1.0",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "3.0.2",
    "node-babel": "^0.1.2",
    "node-sass": "^3.8.0",
    "phantomjs-prebuilt": "^2.1.12",
    "react-addons-test-utils": "^15.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.5.4",
    "sass-loader": "4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "0.19.2",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
