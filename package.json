{
  "name": "redux-async-boilerplate",
  "version": "0.0.0",
  "description": "Redux Async Boilerplate",
  "scripts": {
    "start": "npm run dev",
    "dev": "node -r dotenv/config ./bin/server.js",
    "dev:no-debug": "npm run dev -- --no_debug",
    "lint": "eslint ./src/ --ext .js --ext .jsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=production karma start ./bin/karma.js",
    "test:watch": "watch 'npm run test' ./tests/",
    "clean": "rimraf ./dist",
    "compile": "NODE_ENV=production DEBUG=app:* webpack -p --config ./config/webpack.prod.config.js --progress",
    "deploy": "npm run test && npm run clean && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedasilva/redux-async-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shanedasilva/redux-async-boilerplate/issues"
  },
  "homepage": "https://github.com/shanedasilva",
  "dependencies": {
    "classnames": "^2.2.1",
    "debug": "^2.2.0",
    "dotenv": "^1.2.0",
    "history": "1.17.0",
    "humps": "1.0.0",
    "immutable": "^3.7.5",
    "lodash": "^3.10.1",
    "normalizr": "^1.0.0",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.1",
    "react-router": "^1.0.0-rc1",
    "redux": "^3.0.5",
    "redux-logger": "^2.0.2",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "1.0.2",
    "whatwg-fetch": "^0.10.1",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-loader": "6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "core-js": "2.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.3",
    "expect": "^1.13.4",
    "express": "^4.13.3",
    "file-loader": "0.8.5",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.6.1",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.1",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-babel": "^0.1.2",
